per_cent={'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0} # процентные ставки по вкладам в различных банках
money=int(input("enter deposit amount")) # введите желаемую сумму вклада
deposit=[] # список
for key in per_cent:
 deposit.append(per_cent[key]*money/100)
max_deposit=max(deposit)
print(deposit)
print ("maximum earnings — "+str(max_deposit))  # максимальная сумма, которую вы можете получить
